<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE install PUBLIC "-//Joomla! 1.6//DTD template 1.0//EN" "http://www.joomla.org/xml/dtd/1.6/template-install.dtd">
<extension type="template" version="2.5" client="administrator">
	<name>HUB Admin</name>
	<creationDate>05/24/06</creationDate>
	<author>HUBzero</author>
	<copyright></copyright>
	<license>GNU/GPL</license>
	<version>1.0</version>
	<description>Administration template for Joomla! 1.5</description>
	<files>
		<filename>index.php</filename>
		<filename>css/template.css</filename>
	</files>
	<positions>
		<position>menu</position>
		<position>submenu</position>
		<position>toolbar</position>
		<position>title</position>
		<position>status</position>
		<position>icon</position>
		<position>cp_shell</position>
		<position>cpanel</position>
		<position>debug</position>
	</positions>
	<config>
		<fields name="params">
			<fieldset name="basic">
				<field name="theme" type="list" default="gray" label="TPL_HUBBASICADMIN_FIELD_THEME_LABEL" description="TPL_HUBBASICADMIN_FIELD_THEME_DESC">
					<option value="gray">TPL_HUBBASICADMIN_FIELD_THEME_GRAY</option>
					<option value="steelblue">TPL_HUBBASICADMIN_FIELD_THEME_BLUE_STEEL</option>
					<option value="blue">TPL_HUBBASICADMIN_FIELD_THEME_BLUE</option>
					<option value="bluepowder">TPL_HUBBASICADMIN_FIELD_THEME_BLUE_POWDER</option>
					<option value="aqua">TPL_HUBBASICADMIN_FIELD_THEME_AQUA</option>
					<option value="aquapowder">TPL_HUBBASICADMIN_FIELD_THEME_AQUA_POWDER</option>
					<option value="green">TPL_HUBBASICADMIN_FIELD_THEME_GREEN</option>
					<option value="greensteel">TPL_HUBBASICADMIN_FIELD_THEME_GREEN_STEEL</option>
					<option value="greenpowder">TPL_HUBBASICADMIN_FIELD_THEME_GREEN_POWDER</option>
					<option value="orange">TPL_HUBBASICADMIN_FIELD_THEME_ORANGE</option>
					<option value="red">TPL_HUBBASICADMIN_FIELD_THEME_RED</option>
					<option value="redsteel">TPL_HUBBASICADMIN_FIELD_THEME_RED_STEEL</option>
					<option value="redpowder">TPL_HUBBASICADMIN_FIELD_THEME_RED_POWDER</option>
					<option value="purple">TPL_HUBBASICADMIN_FIELD_THEME_PURPLE</option>
					<option value="purplesteel">TPL_HUBBASICADMIN_FIELD_THEME_PURPLE_STEEL</option>
					<option value="purplepowder">TPL_HUBBASICADMIN_FIELD_THEME_PURPLE_POWDER</option>
				</field>
			</fieldset>
		</fields>
	</config>
</extension>