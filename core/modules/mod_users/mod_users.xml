<?xml version="1.0" encoding="utf-8"?>
<extension type="module" version="1.5" client="administrator">
	<name>mod_users</name>
	<author>HUBzero</author>
	<authorUrl>hubzero.org</authorUrl>
	<authorEmail>support@hubzero.org</authorEmail>
	<copyright>Copyright 2005-2019 HUBzero Foundation, LLC.</copyright>
	<license>http://opensource.org/licenses/MIT MIT</license>
	<description>MOD_USERS_XML_DESCRIPTION</description>
	<files>
		<filename module="mod_users">mod_users.php</filename>
		<filename>users.css</filename>
		<filename>index.html</filename>
		<filename>tmpl/default.php</filename>
	</files>
	<config>
		<fields name="params">
			<fieldset name="basic">
				<field name="limit" type="text" default="25" label="MOD_USERS_LIMIT_LABEL" description="MOD_USERS_LIMIT_DESC" />
				<field name="cache" type="list" default="1" label="MOD_USERS_CACHE_LABEL" description="MOD_USERS_CACHE_DESC">
					<option value="1">MOD_USERS_CACHE_GLOBAL</option>
					<option value="0">MOD_USERS_CACHE_NONE</option>
				</field>
			</fieldset>
		</fields>
	</config>
</extension>