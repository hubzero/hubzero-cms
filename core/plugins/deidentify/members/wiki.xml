wik<?xml version="1.0" encoding="utf-8"?>
<extension version="1.7" type="plugin" group="members">
	<name>Deidentification - Members - Wiki</name>
	<author>HUBzero</author>
	<authorUrl>hubzero.org</authorUrl>
	<authorEmail>support@hubzero.org</authorEmail>
	<copyright>Copyright (c) 2005-2023 The Regents of the University of California.</copyright>
	<license>http://opensource.org/licenses/MIT MIT</license>
	<description>Deidentification a member from the admin panel</description>
	<files>
		<filename plugin="wiki">members.php</filename>
	</files>
	<languages>
		<language tag="en-GB">en-GB.plg_members_wiki.ini</language>
	</languages>
	<config>
		<fields name="params">
			<fieldset name="basic">
				<field name="display_limit" type="text" size="5" default="50" label="Display Limit" description="Number of items to return"/>
			</fieldset>
		</fields>
	</config>
</extension>
